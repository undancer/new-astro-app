---
import CommonMeta from '@/components/common/CommonMeta.astro'
import Favicons from '@/components/common/Favicons.astro'

interface Props {
  title: string
}

const { title } = Astro.props
const lang = 'en'
---

<!DOCTYPE html>
<html lang={lang}>
<head>
  <CommonMeta />
  <title>{title}</title>
  <Favicons />
</head>
<body>
<slot />
</body>
</html>
